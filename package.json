{
  "name": "cars",
  "version": "0.0.0",
  "description": "Cars management factory",
  "engines": {
    "node": "12.22.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Samuel-Ngwarai/cars.git"
  },
  "dependencies": {
    "tslib": "~2.1.0",
    "express": "~4.17.1",
    "body-parser": "~1.19.0",
    "uuid": "~8.3.2",
    "ajv": "~8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.10",
    "jest": "^26.6.3",
    "nodemon": "~2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "tsutils": "^3.20.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"tsc -w -p tsconfig.release.json\" \"nodemon build/src/main.js --delay 1\"",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
